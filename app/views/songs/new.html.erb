<h1>Create a Song</h1>

<%= form_for @song do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br />
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <br />
  <%= f.label :genre_id %>
  <%= f.select :genre_id, @genre_names %>
  <br />

  <%= f.label :notes_1 %>
  <%= f.text_area :notes_1 %>

  <%= f.label :notes_2 %>
  <%= f.text_area :notes_2 %>


  <%= f.submit %>
<% end %>




